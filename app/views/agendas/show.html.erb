<div id="interno8">
<% title "PROFESSORS".singularize %>
<div class="tabs">
 <div id="caixa_titulo">
     <font size="3"><strong>RESERVA DE SALA - SEDUC</strong></font><br/>
 </div>
 <table bgcolor="#F5F5F5" width="100%">
   <tr  bgcolor="#F5F5F5" width="100%">
     <td>
       <fieldset><legend><font  size=3><b> AGENDAMENTO</b></font></legend>
             <table  width="100%"  align="center"  bgcolor="#F5F5F5" >
        <tr>
           <td align="left"><font size=2><b>Local:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.unidade.nome%> <%=h $idreserva %> </font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Data Reserva:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.data.strftime("%d/%m/%Y")%></font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Inicio:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.horai.strftime("%H:%M")%> horas</font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Final:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.horaf.strftime("%H:%M")%> horas</font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Solicitante:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.solicitante%></font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Utilização:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.proposito%></font></td>
        </tr>
        <tr>
           <td align="left"><font size=2><b>Observação:</b></font></td>
           <td align="left"><font size=2><%=h @agenda.obs%></font></td>
        </tr>
      </table>
       </fieldset>
    </td>
  </tr>
  <tr>
    <td colspan="2">

        <table width="100%" align="left" >
           <tr>
              <td width="2"align="left" ><%= button_to 'Fechar', home_path, {:method => "get"}  %></td>
              <td width="2"> <%= button_to("Agenda", agendas_path, :method => 'get',:title => "Voltar" )%></td>
              <td width="6%"align="left" ><%= button_to 'Editar', {:action => "edit", :id =>@agenda}, :method => "get", :title => "Editar"%></td>
              <td ><%= button_to("Imprimir", onclick="javascript:window.print()", :title => "Imprimir") %></td>
           </tr>
        </table>

    </td>
  </tr>
     </table>

</div>
<br/>
</div>







