                </tr>
                 <tr>
                     <td align="left"><b>E-mail: </b></td>
                     <td align="left"><%=h @participanteemail %>  -  <%=h @participante.email %>  </td>
                 </tr>
                 <%if @participante.professor_id.present?%>
                <tr>
                   <td align="left"><b>Unidade: </b></td>
                   <td align="left"><%=h @professor.unidade.nome %></td>
                </tr>
                <tr>
                   <td align="left"><b>Função: </b></td>
                   <td align="left"><%=h @professor.funcao%></td>
                </tr>
                <%end%>
                 <%if @participante.funcionario_id.present?%>
                <tr>
                   <td align="left"><b>Unidade: </b></td>
                   <td align="left"><%=h @funcionario.unidade.nome %></td>
                </tr>
                <tr>
                   <td align="left"><b>Função: </b></td>
                   <td align="left"><%=h @funcionario.funcao%></td>
                </tr>
                <%end%>
                 <tr>
                   <td align="left"><b>Observação: </b></td>
                   <td align="left"><%=h @participante.obs%></td>
                </tr>
        </table>
