<font size="2">
<table class="tabelas"width="40%"align="center">
 <tr>
   <th align="left">
       <font  size=5 color="black"> <b>CONTROLE DE PROTOCOLOS - SEDUC/PMA</b></font>
   </th>
    <td width="5%">
     <center><%= link_to image_tag("/stylesheets/images/excluir.PNG", :size => "20x20"),  homes_path, {:method => "get"}, :title => "Fechar"%></center>
   </td>
 </tr>
 <tr>
   <td colspan="2">
    <fieldset><legend><font  size=4>PROTOCOLO</font></legend>
       <table>
          <tr>
            <td width="1%"></td>
            <td width="1%" valign="Top" width="9%"><b>Protocolo:</b></td>
            <td width="5%"><%=h @prefprotocolo.codigo %></td>
            <td width="1%"align="right"><b>Requerente: </b></td>
            <td width="10%"align="left"><%=h @prefprotocolo.de %></td>
          </tr>
          <tr>
            <td></td>
            <td align="left"><b>Assunto: </b></td>
            <td colspan="3"align="left"><%=h @prefprotocolo.assunto%></td>
          </tr>
          <tr>
            <td></td>
            <td align="left"><b>Destinatario: </b></td>
            <td colspan="3"align="left"><%=h  @prefprotocolo.destino %>
                &nbsp&nbsp<b>Para:</b> <%=h @prefprotocolo.para %>
                &nbsp&nbsp<b>em:</b> <%=h @prefprotocolo.data.strftime("%d/%b/%Y")%>
             </td>
          </tr>
          <tr>
            <td></td>
            <td align="left" valign="top"><b>Despachos: </b></td>
            <td align="left" colspan="3"> <%= render :partial => 'despachos'  %></td>
          </tr>
           <% if @prefprotocolo.encerrado != 0 %>
              <tr>
                <td></td>
                <td colspan="4"><b>Encerrado em</b>  <%=h @prefprotocolo.data_encerramento.strftime("%d de %B de %Y") %></td>
              </tr>
            <% end %>

       </table>
    </fieldset>
    <div class="botao">
      <table>
           <tr>
               <td width="6%"align="left" > <%= link_to image_tag("/stylesheets/images/voltar.png", :size => "40x40"), homes_path, :title => "Voltar" %></td>
              <td width="2%"align="left" ><%= link_to image_tag("/stylesheets/images/buscar.png", :size => "30x30"),  prefprotocolos_path, :title => "Ofícios Cadastrados"%></td>
              <td align="left"><%= link_to image_tag("/stylesheets/images/consulta1.png", :size => "40x40"), consulta_prefprotocolos_path, :title => "Consulta Ofícios" %></td>
            </tr>
      </table>
   </div>
  </td>
 </tr>
</table>
</font>
