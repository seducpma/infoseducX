<% c=0 %>
<% @prefprotocolo.despachos.each do |despacho| %>
      <b><%= c=c+1%>.</b><b><u>Despacho:</u></b> <%="#{despacho.despacho}" %>  <br/>
      &nbsp <b> de:</b> <%="#{despacho.procedencia}" %> <b>em</b> <%="#{despacho.data_saida.strftime("%d de %B de %Y")}" %><br/>
      &nbsp <b> destino:</b> <%="#{despacho.destino}" %> <b>para:</b> <%="#{despacho.para}"%><br/>
      &nbsp ---------------------------------------------------------<br/>



<%end%>

