<% form_for @prefprotocolo do |f| %>
<%= f.error_messages %>
<font  size="2">
<table class="tabelas" align="center">
 <tr>
    <th align="left">
       <font size=5 color="black"> <b>  CONTROLE DE PROTOCOLO - SEDUC/PMA</b></font>
    </th>
    <td width="5%">
     <center><%= link_to image_tag("/stylesheets/images/excluir.PNG", :size => "20x20"),  homes_path, {:method => "get"}, :title => "Fechar"%></center>
   </td>
 </tr>
 <tr>
   <td colspan="2">
     <table width="100%">
        <tr>
           <td width="10%"align="left"><font size="2"><b>Protocolo:</b></font></td>
           <td width="10%"align="left"><font size="2"><%=h @prefprotocolo.codigo %></font></td>
           <td width="10%"align="right"><font size="2"><b>Data:</b></font></td>
           <td align="left"><font size="2"><%=h @prefprotocolo.data.strftime("%d de %B de %Y") %></font></td>

        </tr>
        <tr>
           <td align="left"><font size="2"><b>Requerente:</b></font></td>
           <td align="left" colspan="3"><font size="2"><%=h @prefprotocolo.de %></font></td>
        </tr>
        <tr>
           <td align="left"><font size="2"><b>Assunto:</b></font></td>
           <td align="left" colspan="3"><font size="2"><%=h @prefprotocolo.assunto%></font></td>
        </tr>
        <tr>
           <td align="left"><font size="2"><b>Destino:</b></font></td>
           <td align="left"><font size="2"><%=h  @prefprotocolo.destino %></font></td>
           <td align="right"><font size="2"><b>Para:</b></font></td>
           <td align="left"><font size="2"> <%=h @prefprotocolo.para %></font></td>

        </tr>
        <tr>
          <td align="left" valign="top"><font size="2"><b>Despachos:</b></font></td>
          <td colspan="3"><div id="dados"> <%= render :partial => 'despachos'  %></div></td>
        </tr>
        <% if  @prefprotocolo.encerrado == 1  %>
        <tr>
           <td align="left"><b>Encerrado:</b></td>
           <td align="left" colspan="3"> <b>em</b> <%= f.check_box :encerrado %> EM <%= f.date_select(:data_encerramento, :order => [:day, :month, :year]) %></td>
        </tr>
        <%end%>

     </table>
   </td>
 </tr>
 <tr>
   <td colspan="2">
      <div class="botao">
        <table width="40%" align="left">
           <tr>
             <div id='Certeza'>
               <td width="6%"align="left" > <%= link_to image_tag("/stylesheets/images/voltar.png", :size => "40x40"), homes_path, :title => "Voltar" %></td>
              </div>
              <td width="6%" align="left"><input type="image" src="/stylesheets/images/salvar.png" title="Gravar"></td>
              <td width="2%" align="left" ><%= link_to image_tag("/stylesheets/images/buscar.png", :size => "30x30"), prefprotocolos_path, :title => "Protocolos Cadastrados"%></td>
<% end %>     <td width="2%" align="left"><%= link_to image_tag("/stylesheets/images/consulta1.png", :size => "40x40"), consulta_prefprotocolos_path, :title => "Consulta Protocolos" %></td>
              <td align="left"><%= link_to image_tag("/stylesheets/images/write.png", :size => "30x30"), "#inline_despacho", :class =>"inline", :title => "Despachar" %></td>
            </tr>
        </table>
      </div>

   </td>
  </tr>
 </table>
 </font>
<br/>

    <%= render "despacho" %>
