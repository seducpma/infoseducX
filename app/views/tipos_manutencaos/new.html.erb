<br/><br/>
<table class="tabelas" align="center" width="100%">
  <tr>
     <th colspan="2" align="left">
       <font  size=5 color="black"> <b>  TIPO DE SERVIÇO DE MANUTENÇÃO</b></font>
    </th>
   </tr>
 <tr>
   <td>
    <font  size="2">
     <table width="100%"  >
      <tr>
        <td width="50%" valign="top">
          <% form_for(@tipos_manutencao) do |f| %>
            <fieldset><legend><font  size=4> Cadastrar Serviço</font></legend>
             <table >
               <tr>
                 <td colspan="3"width="20%"align="left" ><b>Serviço:</b>
                     <%= f.text_field :servico %>
                       <%= observe_field 'tipos_manutencao_servico',
                          :frequency => 0.1,
                          :update => "cadastrados",

                          :url => { :action => 'lista' } %>
                </td>
              </tr>
               <tr>
                <div class="botao">
                   <div id='Certeza'>
                     <td width="1%" align="left" > <%= link_to image_tag("/stylesheets/images/voltar.png", :size => "40x40"), homes_path, :title => "Voltar" %></td>
                   </div>
                   <td width="1%"align="left"><input type="image" src="/stylesheets/images/salvar.png" title="Gravar"></td>
                   <td align="left" ><%= link_to image_tag("/stylesheets/images/imprimir.png", :size => "30x30"), onclick="javascript:window.print()", :title => "Imprimir" %></td>
                 </div>
              </tr>
             </table>
            </fieldset>
           <%end%>
         </td>
         <td width="50%" valign="top">
             <fieldset><legend><font  size=4> Tipos Serviços Cadastrados</font></legend>
              <table width="100%">
               <tr>
                  <td align="left">
                    <div id="cadastrados"></div>
                  </td>
               </tr>
              </table>
             </fieldset>
           </td>
      </tr>
    </table>

    </font>
  </td>
</table>
<br/>


