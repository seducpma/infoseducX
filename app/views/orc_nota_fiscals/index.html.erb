    <td><%= link_to 'Show', orc_nota_fiscal %></td>
    <td><%= link_to 'Edit', edit_orc_nota_fiscal_path(orc_nota_fiscal) %></td>
    <td><%= link_to 'Destroy', orc_nota_fiscal, :confirm => 'Are you sure?', :method => :delete %></td>
