<h1>Editing cursos_inscricao</h1>

<% form_for(@cursos_inscricao) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :curso %><br />
    <%= f.text_field :curso %>
  </p>
  <p>
    <%= f.label :inscricao %><br />
    <%= f.text_field :inscricao %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @cursos_inscricao %> |
<%= link_to 'Back', cursos_inscricaos_path %>