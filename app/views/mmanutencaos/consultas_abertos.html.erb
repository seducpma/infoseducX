<div id="interno2">
<% title "UNIDADES", :id =>"unidades" %>
 <div class="tabs">
   <div id="caixa_titulo">
    <font size="3"><strong>MANUTENÇÃO GERAL - CHAMADOS EM ABERTOS<%session[:encerrados]= 1 %></strong></font><br/>
   </div>
  <div class="tabs">
    <% form_remote_tag :url => consulta_abertos_unidade_path, :method => :get,:loading => "$('#status').show()" , :success => "$('#status').hide()" do %>
    <table width="100%" align="center" bgcolor="#DCDCDC" >
          <tr>
            <td>
              <a href="#" class="filtro_nome"><%= radio_button_tag(:type_of, "1") %></a>Serviço
              <a href="#" class="filtro_status"><%= radio_button_tag(:type_of, "4") %></a>Status
              <a href="#" class="filtro"><%= radio_button_tag(:type_of, "2") %></a>Unidade
              <a href="#" class="filtro_data"><%= radio_button_tag(:type_of, "5") %></a>Data
              <a href="#" class="filtro_tipo"><%= radio_button_tag(:type_of, "3") %></a>Palavras 
            </td>
         </tr>
         <tr>
           <td><div class="consulta" style="display:none"> <%= render "consultaUnidade" %> </div>
               <div class="status" style="display:none"> <%= render "consultaStatus" %> </div>
              <div class="consulta1" style="display:none"> <%= render "consultaServico" %> </div>
             <div class="consulta_data" style="display:none"> <%= render "consultaData" %> </div>
              <div class="consulta2" style="display:none"> <%= render "consultaTodos" %> </div></td>
               
        </tr>
        </table>
    <% end %>
    <br/>
    <hr/>
    <div id="abertos"><div id='status' style='display:none;' align="center" ><%= image_tag("/stylesheets/images/loading.gif") %></div></div>
    <br/>

  <table>
    <tr>
     <td width="6%"align="left" ><%= button_to 'Fechar', home_path, {:method => "get"}  %></td>
     <td align="left"><%= button_to 'Nova Consulta', consultas_encerrados_path, :title => "Nova Consulta" %></td>
    </tr>
  </table>
 </div>
 </div>
  <br/>
</div>

