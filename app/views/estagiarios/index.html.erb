<h1>Listing estagiarios</h1>

<table>
  <tr>
    <th>Unidade</th>
    <th>Regiao</th>
    <th>Nome</th>
    <th>Rg</th>
    <th>Cpf</th>
    <th>Endereco</th>
    <th>Num</th>
    <th>Complemento</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Fone</th>
    <th>Cel</th>
    <th>Email</th>
    <th>Data nasc</th>
    <th>Faculdade</th>
    <th>Matricula</th>
    <th>Periodo est</th>
    <th>Carga horaria</th>
    <th>Horario</th>
    <th>Responsavel</th>
    <th>Data ingresso</th>
    <th>Data termino</th>
    <th>Data admissao</th>
    <th>Matriculapma</th>
    <th>Data desliga</th>
    <th>Locacao</th>
    <th>Periodo trab</th>
    <th>Aval</th>
    <th>Obs</th>
    <th>Flag</th>
    <th>Desligado</th>
    <th>Etinerante</th>
    <th>Photo file name</th>
    <th>Photo content type</th>
    <th>Photo file size</th>
    <th>Tipo</th>
  </tr>

<% @estagiarios.each do |estagiario| %>
  <tr>
    <td><%=h estagiario.unidade %></td>
    <td><%=h estagiario.regiao %></td>
    <td><%=h estagiario.nome %></td>
    <td><%=h estagiario.RG %></td>
    <td><%=h estagiario.CPF %></td>
    <td><%=h estagiario.endereco %></td>
    <td><%=h estagiario.num %></td>
    <td><%=h estagiario.complemento %></td>
    <td><%=h estagiario.bairro %></td>
    <td><%=h estagiario.cidade %></td>
    <td><%=h estagiario.fone %></td>
    <td><%=h estagiario.cel %></td>
    <td><%=h estagiario.email %></td>
    <td><%=h estagiario.data_nasc %></td>
    <td><%=h estagiario.faculdade %></td>
    <td><%=h estagiario.matricula %></td>
    <td><%=h estagiario.periodo_est %></td>
    <td><%=h estagiario.carga_horaria %></td>
    <td><%=h estagiario.horario %></td>
    <td><%=h estagiario.responsavel %></td>
    <td><%=h estagiario.data_ingresso %></td>
    <td><%=h estagiario.data_termino %></td>
    <td><%=h estagiario.data_admissao %></td>
    <td><%=h estagiario.matriculaPMA %></td>
    <td><%=h estagiario.data_desliga %></td>
    <td><%=h estagiario.locacao %></td>
    <td><%=h estagiario.periodo_trab %></td>
    <td><%=h estagiario.aval %></td>
    <td><%=h estagiario.obs %></td>
    <td><%=h estagiario.flag %></td>
    <td><%=h estagiario.desligado %></td>
    <td><%=h estagiario.etinerante %></td>
    <td><%=h estagiario.photo_file_name %></td>
    <td><%=h estagiario.photo_content_type %></td>
    <td><%=h estagiario.photo_file_size %></td>
    <td><%=h estagiario.tipo %></td>
    <td><%= link_to 'Show', estagiario %></td>
    <td><%= link_to 'Edit', edit_estagiario_path(estagiario) %></td>
    <td><%= link_to 'Destroy', estagiario, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

