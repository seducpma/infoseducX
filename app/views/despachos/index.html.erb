<h1>Listing despachos</h1>

<table>
  <tr>
    <th>Prefprotocolo</th>
    <th>Procedencia</th>
    <th>Data recebimento</th>
    <th>Para</th>
    <th>Despacho</th>
    <th>Destino</th>
    <th>Data saida</th>
  </tr>

<% @despachos.each do |despacho| %>
  <tr>
    <td><%=h despacho.prefprotocolo %></td>
    <td><%=h despacho.procedencia %></td>
    <td><%=h despacho.data_recebimento %></td>
    <td><%=h despacho.para %></td>
    <td><%=h despacho.despacho %></td>
    <td><%=h despacho.destino %></td>
    <td><%=h despacho.data_saida %></td>
    <td><%= link_to 'Show', despacho %></td>
    <td><%= link_to 'Edit', edit_despacho_path(despacho) %></td>
    <td><%= link_to 'Destroy', despacho, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New despacho', new_despacho_path %>