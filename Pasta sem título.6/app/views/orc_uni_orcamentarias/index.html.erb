<div id="interno1">
<% title "GERENCIAMENTO DE USUÁRIOS", :id =>"users" %>
 <div class="tabs">
   <div id="caixa_titulo">
    <font size="3"><strong>ORÇAMENTO SEDUC/PMA - UNIDADE ORÇAMENTARIA</strong></font><br/>
   </div>
  <div class="tabs">
    <br/>
    <table bgcolor="#DCDCDC" width="100%">
        <tr bgcolor="#DCDCDC" >
            <td align="center"><b>Codigo</b></td>
            <td align="center"><b>Ano </b></td>
            <td align="center"><b>Un.Orçamentaria</b></td>
            <td align="center"><b>Despesa</b></td>
          </tr>
          <% @orc_uni_orcamentarias.each  do |orcamentaria| %>
            <tr  bgcolor="#F5F5F5">
              <td><%=h orcamentaria.codigo %></td>
              <td align="center"><%=h orcamentaria.ano %></td>
              <td><%=h orcamentaria.descricao %></td>
              <td><%=h orcamentaria.orc_uni_despesa.descricao %></td>

              <td><%= button_to 'Visualizar',{:action => "show", :id =>orcamentaria}, :method => "get", :title => "Visualizar"%> </td>
              <td><%= button_to 'Editar',{:action => "edit", :id =>orcamentaria}, :method => "get", :title => "Editar"%> </td>
              <td><%= button_to 'Excluir', orcamentaria, :confirm => 'Confirma Exclusao?', :method => :delete, :title => "Excluir" %></td>
            </tr>
          <% end %>
    </table>





<div id='status' style='display:none;' align="center" ><%= image_tag("/stylesheets/images/loading.gif") %></div>  </div>
    <br/>
  <table>
       <tr>
         <td width="2%"align="left"  ><%= button_to('Fechar', home_path, :method => "get", :title => "Fechar")%></td>
         <td align="left" ><%= button_to('Unidades Orçamentarias', orc_uni_orcamentarias_path, :method => "get",  :title => "Unidades Orçamentariaa")%></td>

      </tr>
  </table>
