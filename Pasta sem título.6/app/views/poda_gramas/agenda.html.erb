
<div id="interno2">
<% title "ALUNOS".singularize %>

 <div id="caixa_titulo" class ="invisible_to_print ">
     <font size="3"><strong>AGENDA DE PODA DE GRAMA</strong></font><br/>
 </div>

      <div class="tabs">
   <% form_remote_tag :url => agenda_poda_grama_path, :method => :get,:loading => "$('#status').show()" , :success => "$('#status').hide()" do %>
      <fieldset>
          <table width="100%" align="center" bgcolor="#DCDCDC">
          <tr>
            <td>
               <a href="#" class="filtro_agendamento"><%= radio_button_tag(:type_of, "1") %></a>Solicitações Poda de Grama
               <a href="#" class="filtro_agenda"><%= radio_button_tag(:type_of, "2") %></a>Agendamento Execuções
               <a href="#" class="filtro_executado"><%= radio_button_tag(:type_of, "3") %></a>Podas de Grama Executadas
               <a href="#" class="filtro_executado"><%= radio_button_tag(:type_of, "4") %></a>Podas de Grama Agendadas mas NÃO Executados
            </td>
         </tr>

          <tr>
            <td width ="14%" align="right">
              <%= submit_tag "Visualizar", :class => "btn_filtrar" %>
            </td>
        </tr>
        <tr bgcolor="#F5F5F5">
            <td with ="100%">
                <div id="calendario"></div>
            </td>

          </tr>

       </table>
   <% end %>

      </fieldset>
       <br/>
       <table>
    <tr>
     <td width="6%"align="left" ><%= button_to 'Fechar', home_path, {:method => "get"}  %></td>
     <td align="left"><%= button_to 'Nova Consulta', agenda_poda_gramas_path, :title => "Nova Consulta" %></td>

    </tr>
  </table>
 </div>



<br/>
</div>


